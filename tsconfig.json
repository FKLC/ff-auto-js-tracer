{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 23",
  "_version": "23.0.0",

  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "NodeNext",
    "target": "ESNext",

    "strict": true,
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "nodenext"
  }
}